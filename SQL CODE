SELECT * FROM EMP
WHERE JOB='MANAGER'
SELECT * FROM EMP
WHERE JOB='MANAGER' AND SAL<='3000' 
SELECT MAX(SAL) FROM EMP
WHERE JOB='MANAGER'
SELECT * FROM EMP
WHERE JOB='MANAGER' OR DEPTNO='30'
SELECT * FROM EMP
WHERE ENAME IN('MARTIN','JAMES')
SELECT MAX(HIREDATE) AS MAXX,MIN(HIREDATE) AS MINN FROM EMP
SELECT * FROM EMP
WHERE HIREDATE BETWEEN  '1/1/1981' AND '1/1/1982'
SELECT * FROM EMP
WHERE ENAME LIKE 'K__G'
SELECT * FROM EMP
WHERE ENAME LIKE 'K%G'
SELECT * FROM EMP
WHERE SAL<(SELECT AVG(SAL) FROM EMP WHERE JOB='MANAGER')


